{
  "name": "sugarcube-e2e-test",
  "version": "0.0.1",
  "description": "Write end to end tests for your sugarcube project without firing up a browser.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jasmine",
    "compile-typescript": "echo \"compile ts\" && tsc && cp -r src/internal/declarations dist/internal/declarations && cp src/internal/html.tpl dist/internal/html.tpl",
    "only-build": "npm run compile-typescript",
    "clean": "rm -rf tscbuild; rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BawdyInkSlinger/sugarcube-e2e-test.git"
  },
  "keywords": [
    "testing",
    "sugarcube",
    "twine"
  ],
  "author": "Bawdy Ink Slinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BawdyInkSlinger/sugarcube-e2e-test/issues"
  },
  "homepage": "https://github.com/BawdyInkSlinger/sugarcube-e2e-test#readme",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jasmine": "^5.1.1",
    "@types/jquery": "^3.5.25",
    "@types/jsdom": "^21.1.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/seedrandom": "^2.4.28",
    "eslint": "^8.52.0",
    "jasmine": "^5.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0"
  },
  "dependencies": {
    "glob": "^10.3.10",
    "jquery": "^3.7.1",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "prettier": "^3.0.3",
    "seedrandom": "2.3.3"
  }
}
